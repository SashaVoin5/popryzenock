<Window x:Class="popryzenock.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:popryzenock"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        Icon="Resources/logo.png">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="43*"/>
            <RowDefinition Height="360*"/>
            <RowDefinition Height="31*"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0 " Background="#F9969E" Height="auto" Width="auto">
            <Image Width="35" Height="32" HorizontalAlignment="Left" Margin="5" Source="Resources/logo.png"/>

        </StackPanel>
        <StackPanel Grid.Row="2" Background="#F9969E" Height="auto" Width="auto" Margin="0,4,0,0">

        </StackPanel>
        <Frame x:Name="frame" Height="auto" Width="auto" LoadCompleted="frame_LoadCompleted" Margin="0,33,0,0" Grid.RowSpan="2"/>


    </Grid>
</Window>
